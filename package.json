{
  "name": "canechair.parts",
  "version": "0.1.0",
  "author": "Simon X.L.",
  "scripts": {
    "prestart": "npm run generate:documents",
    "start": "npm-run-all --parallel security-check start:dev generate:documents-watch",
    "start:dev": "babel-node CCPSettings/Server/CCPSettings.Server.Dev.js",
    "security-check": "nsp check",
    "generate:documents": "babel-node CCPScripts/CCPScripts.GenerateDocuments.js",
    "generate:documents-watch": "npm run generate:documents -- --watch",
    "build": "babel-node CCBuildScripts/CCBuildScripts.BuildProd.js",
    "test": "babel-node CCBuildScripts/CCBuildScripts.Test.js --env=jsdom"
  },
  "keywords": [
    "components",
    "react",
    "react-component",
    "toolkit"
  ],
  "dependencies": {
    "highlight.js": "^9.12.0",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "colors": "^1.1.2",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "open": "0.0.5",
    "react-docgen": "^2.20.0",
    "react-hot-loader": "^3.1.3",
    "style-loader": "^0.19.1",
    "webpack": "3.8.1"
  }
}
